{
  "source": {
    "include": "src"
  },
  "opts": {
    "destination": "./web/doc/",
    "encoding": "utf8",
    "recurse": true,
    "template": "node_modules/minami",
    "readme": "README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "includeDate": false
    }
  }
}
